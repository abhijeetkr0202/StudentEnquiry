<%- include ('..\\views\\partials\\header')%>
<div class="ui main text container segment">
    <div class="ui huge header">Add new Student data</div>
        <form class="ui form"  method="post">
            <div class="field">
                <label>USN</label>
                <input type="text" maxlength="10" onkeyup="this.value = this.value.toUpperCase();" required name="studentusn" placeholder="USN">
            </div>
            <div class="field">
                <label>Name</label>
                <input type="text"  maxlength="25" onkeyup="this.value = this.value.toUpperCase();" required name="studentname" placeholder="Name" >
            </div>
            <div class="field">
                <label>Branch</label>
                <input type="text"  maxlength="3" onkeyup="this.value = this.value.toUpperCase();" required name="studentbranch" placeholder="Branch" >
            </div>
            <div class="field">
                <label>Course</label>
                <input type="text"  maxlength="5" onkeyup="this.value = this.value.toUpperCase();" required name="studentcourse" placeholder="Course">
            </div>
            <div class="field">
                <label>Gender</label>
                <input type="text"  maxlength="1" onkeyup="this.value = this.value.toUpperCase();" required name="studentgender" placeholder="Gender" >
            </div>
            <div class="field">
                <label>Semester</label>
                <input type="number"  maxlength="1" onkeyup="this.value = this.value.toUpperCase();" required name="studentsemester" placeholder="Semester" >
            </div>
            <div class="field">
                <label>Passing Year</label>
                <input type="text"  maxlength="4" onkeyup="this.value = this.value.toUpperCase();" required name="studentpassingyear" placeholder="Passing Year" >
            </div>
            <div class="field">
                <label>GPA</label>
                <input type="number" step="0.01" onkeyup="this.value = this.value.toUpperCase();" required name="studentgpa" placeholder="GPA" >
            </div>
            <div class="field">
                <label>12th Percentage</label>
                <input type="number"  step="0.01" maxlength="5" onkeyup="this.value = this.value.toUpperCase();" required name="studentpercentage" placeholder="Enter Marks obtained in PUC">
            </div>
            <div class="field">
                <label>Email</label>
                <input type="text"  maxlength="25" required name="studentemail" placeholder="Email" >
            </div>
            <div class="field">
                <label>Date of Birth</label>
                <span><input type="date"  maxlength="10" required name="studentdob" placeholder="Date of Birth">(MM/DD/YYYY)</span>
            </div>
            <div class="field">
                <label>Blood Group</label>
                <input type="text"  maxlength="3" onkeyup="this.value = this.value.toUpperCase();" required name="studentbloodgroup" placeholder="BloodGroup" >
            </div>
            <div class="field">
                <label>Nationality</label>
                <input type="text"  maxlength="15" onkeyup="this.value = this.value.toUpperCase();" required name="studentnationality" placeholder="Nationality" >
            </div>
            <div class="field">
                <label>Father's Name</label>
                <input type="text"  maxlength="15" onkeyup="this.value = this.value.toUpperCase();" required name="studentfathersname" placeholder="Father's Name" >
            </div>
            <div class="field">
                <label>Mother's Name</label>
                <input type="text"  maxlength="15" onkeyup="this.value = this.value.toUpperCase();" required name="studentmothersname" placeholder="Mother's Name" >
            </div>
            <div class="field">
                <label>Student's Contact Number</label>
                <input type="text"  maxlength="10" onkeyup="this.value = this.value.toUpperCase();" required name="studentcontact" placeholder="student's Contact Number">
            </div>
            <div class="field">
                <label>Parent's Contact Number</label>
                <input type="text"  maxlength="10" onkeyup="this.value = this.value.toUpperCase();" required name="studentparentcontact" placeholder="Parents Contact Number" >
            </div>
            <div class="field">
                <label>Address</label>
                <input type="text"  maxlength="30" onkeyup="this.value = this.value.toUpperCase();" required name="studentaddress" placeholder="Address" >
            </div>
            <div class="field">
                <label>Parent's Income</label>
                <input type="number"  maxlength="15" onkeyup="this.value = this.value.toUpperCase();" required name="studentincome" placeholder="Income">
            </div>
        
           
            
            
            <input class="ui blue big basic button" type="submit">
        </form>
    
    </div>
<%- include ('..\\views\\partials\\footer')%>